{
    "name": "tipoff/support",
    "description": "Laravel Package for core support features used in Ecommerce packages",
    "keywords": [
        "tipoff",
        "support"
    ],
    "homepage": "https://github.com/tipoff/support",
    "license": "MIT",
    "authors": [
        {
            "name": "Tipoff",
            "email": "packages@tipoff.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "spatie/laravel-package-tools": "^1.1",
        "illuminate/contracts": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.9"
    },
    "autoload": {
        "psr-4": {
            "Tipoff\\Support\\": "src",
            "Tipoff\\Support\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tipoff\\Support\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tipoff\\Support\\SupportServiceProvider"
            ],
            "aliases": {
                "Support": "Tipoff\\Support\\SupportFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
