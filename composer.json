{
    "name": "tipoff/support",
    "description": "Laravel Package for core support features used in Ecommerce packages",
    "keywords": [
        "tipoff",
        "support"
    ],
    "homepage": "https://github.com/tipoff/support",
    "license": "MIT",
    "authors": [
        {
            "name": "Tipoff",
            "email": "packages@tipoff.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "beberlei/assert": "^3.3",
        "cakephp/chronos": "^1.2.3|^2.0",
        "illuminate/cache": "^8.0",
        "illuminate/collections": "^8.0",
        "illuminate/contracts": "^8.0",
        "illuminate/database": "^8.18",
        "laravel/nova": "^3.20",
        "marc-mabe/php-enum": "^4.4",
        "nesbot/carbon": "^2.44",
        "spatie/laravel-fractal": "^5.8",
        "spatie/laravel-package-tools": "^1.4"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3",	
        "spatie/laravel-ray": "^1.9",	
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Tipoff\\Support\\": "src",
            "Tipoff\\Support\\Database\\Factories\\": "database/factories"
        },
        "files": [
            "src/Helpers/Test.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tipoff\\Support\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tipoff\\Support\\SupportServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
